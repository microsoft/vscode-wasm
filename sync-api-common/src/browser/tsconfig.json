{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "es2020",
        "lib": [
            "webworker",
            "es2020"
        ],
        "rootDir": ".",
        "types": [],
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "declaration": true,
        "stripInternal": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "../../lib/browser",
        "tsBuildInfoFile": "../../lib/browser/compile.tsbuildInfo",
        "incremental": true,
        "composite": true
    },
    "include": [
        "."
    ],
    "exclude": [
        "test"
    ],
    "references": [
        {
            "path": "../common/tsconfig.json"
        }
    ]
}