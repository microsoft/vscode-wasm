{
    "compilerOptions": {
        "rootDir": ".",
        "skipLibCheck": true,
        "lib": [
            "es2020",
            "webworker"
        ],
        "types": [
            "node",
            "vscode"
        ],
        "module": "Node16",
        "moduleResolution": "Node16",
        "outDir": "./out",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "declaration": true,
        "stripInternal": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}
