{
	"name": "@vscode/wasm-wasi-tests",
	"private": true,
	"version": "0.9.0",
	"author": "MS",
	"license": "MIT",
	"publisher": "ms-vscode",
	"description": "Test cases for the WASM WASI API",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/microsoft/vscode-wasm.git"
	},
	"bugs": {
		"url": "https://github.com/microsoft/vscode-wasm/issues"
	},
	"homepage": "https://github.com/microsoft/vscode-wasm#readme",
	"scripts": {
		"clean": "node ../node_modules/rimraf/bin.js lib",
		"compile": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.json",
		"compile:publish": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.publish.json",
		"watch": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.watch.json -w",
		"lint": "node ../node_modules/eslint/bin/eslint.js --ext ts src",
		"test": "npm run test:node",
		"test:node": "node ../node_modules/mocha/bin/_mocha",
		"symlink:publish": "node ../build/bin/symlink-wasm-wasi-tests-publish.js",
		"all:publish": "git clean -xfd . && npm install && npm run symlink:publish && npm run compile:publish && npm run lint && npm run test"

	},
	"dependencies": {
		"@vscode/sync-api-client": "0.9.0",
		"vscode-uri": "3.0.7"
	},
	"devDependencies": {
		"@types/uuid": "9.0.0",
		"uuid": "9.0.0"
	}
}