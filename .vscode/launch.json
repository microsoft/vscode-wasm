{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Python Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/testbeds/python"],
			"outFiles": ["${workspaceRoot}/testbeds/python/out/**/*.js"],
			"sourceMaps": true
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Rust Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/testbeds/rust"],
			"outFiles": ["${workspaceRoot}/testbeds/rust/out/**/*.js"],
			"sourceMaps": true,
			"preLaunchTask": "Build Rust Example"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "PHP Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/testbeds/php"],
			"outFiles": ["${workspaceRoot}/testbeds/php/out/**/*.js"],
			"sourceMaps": true,
			"preLaunchTask": "Build PHP Example"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Ruby Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/testbeds/ruby"],
			"outFiles": ["${workspaceRoot}/testbeds/ruby/out/**/*.js"],
			"sourceMaps": true,
			"preLaunchTask": "Build Ruby Example"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "C Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/wasm-wasi/example"],
			"outFiles": ["${workspaceRoot}/wasm-wasi/example/**/*.js"],
			"sourceMaps": true
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "C++ Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/testbeds/cpp"],
			"outFiles": ["${workspaceRoot}/testbeds/cpp/out/**/*.js"],
			"sourceMaps": true,
			"preLaunchTask": "Build C++ Example"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "CoreUtils Testbed",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}/testbeds/coreutils"],
			"outFiles": ["${workspaceRoot}/testbeds/coreutils/out/**/*.js"],
			"sourceMaps": true,
			"preLaunchTask": "Build CoreUtils Example"
		},
		{
			"request": "launch",
			"type": "node",
			"name": "test:common",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": ["--timeout", "999999"],
			"cwd": "${workspaceRoot}/sync-api-common",
			"runtimeExecutable": null,
			"runtimeArgs": [],
			"env": { },
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/sync-api-common/lib/**/*.js"],
			"preLaunchTask": "npm: watch"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "test:service",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/sync-api-tests",
				"--extensionTestsPath=${workspaceFolder}/sync-api-tests/lib",
				"${workspaceFolder}/tmp"
			],
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/sync-api-tests/lib/**/*.js"],
			"preLaunchTask": "npm: watch"
		},
		{
			"request": "launch",
			"type": "node",
			"name": "test:wasm-wasi",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": ["--timeout", "999999"],
			"cwd": "${workspaceRoot}/wasm-wasi-tests",
			"runtimeExecutable": null,
			"runtimeArgs": [],
			"env": { },
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/wasm-wasi-tests/lib/**/*.js"],
			"preLaunchTask": "npm: watch"
		}
	]
}